{
    "name": "solital/vinci-console",
    "description": "Console component for Solital Framework",
    "license": "MIT",
    "homepage": "http://solitalframework/",
    "keywords": [
        "framework",
        "php",
        "framework-php",
        "console",
        "vinci-console",
        "php-cli",
        "php-command-line"
    ],
    "require": {
        "php": "^8.3",
        "brenno-duarte/modern-php-exception": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "phpstan/phpstan": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Solital\\Core\\Console\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Solital\\Core\\Console\\Tests\\": "tests/"
        }
    }
}
