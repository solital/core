{
    "name": "solital/vinci-console",
    "description": "Console component for Solital Framework",
    "license": "MIT",
    "homepage": "http://solitalframework/",
    "keywords": [
        "framework",
        "php",
        "framework-php",
        "console",
        "vinci-console",
        "php-cli",
        "php-command-line"
    ],
    "require": {
        "php": "^8.0",
        "brenno-duarte/modern-php-exception": "^1.1",
        "bvanhoekelen/performance": "^2.5",
        "codedungeon/php-cli-colors": "^1.12",
        "nunomaduro/collision": "^5.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Solital\\Core\\Console\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Solital\\Core\\Console\\Test\\": "tests/"
        }
    },
    "bin": "vinci"
}
