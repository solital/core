{
    "name": "solital/vinci-console",
    "description": "Console component for Solital Framework",
    "license": "MIT",
    "homepage": "http://solitalframework/",
    "keywords": [
        "framework",
        "php",
        "framework-php",
        "console",
        "vinci-console",
        "php-cli",
        "php-command-line"
    ],
    "require": {
        "php": "^8.3",
        "brenno-duarte/modern-php-exception": "3.*",
        "codedungeon/php-cli-colors": "^1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpstan/phpstan": "1.*",
        "symfony/var-dumper": "7.*"
    },
    "autoload": {
        "psr-4": {
            "Solital\\Core\\Console\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Solital\\Core\\Console\\Test\\": "tests/"
        }
    }
}
