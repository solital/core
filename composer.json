{
  "name": "solital/core",
  "description": "Official core of Solital framework",
  "license": "MIT",
  "homepage": "http://solitalframework/",
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "brenno-duarte/modern-php-exception": "1.*",
    "egulias/email-validator": "^3.1",
    "fig/event-dispatcher-util": "^1.1",
    "matthiasmullie/minify": "^1.3",
    "phpmailer/phpmailer": "^6.3",
    "psr/container": "1.*",
    "psr/event-dispatcher": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/log": "1.1.*",
    "psr/simple-cache": "1.*",
    "respect/validation": "^2.2",
    "solital/katrina": "1.*",
    "symfony/var-dumper": "^5.2",
    "symfony/var-exporter": "^5.2",
    "vlucas/phpdotenv": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Solital\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Solital\\Test\\": "tests/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "composer dump-autoload -o"
    ],
    "post-install-cmd": [
      "composer dump-autoload -o"
    ],
    "post-update-cmd": [
      "composer dump-autoload -o"
    ]
  }
}