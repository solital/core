{
    "name": "solital/core",
    "description": "Official core of Solital framework",
    "license": "MIT",
    "homepage": "http://solitalframework.rf.gd/",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "brenno-duarte/modern-php-exception": "2.*",
        "brenno-duarte/php-secure-password": "2.*",
        "codedungeon/php-cli-colors": "1.*",
        "delight-im/http": "2.*",
        "egulias/email-validator": "4.*",
        "fig/event-dispatcher-util": "1.*",
        "matthiasmullie/minify": "1.*",
        "nette/php-generator": "4.*",
        "phpmailer/phpmailer": "6.*",
        "psr/cache": "3.*",
        "psr/container": "2.*",
        "psr/event-dispatcher": "1.*",
        "psr/http-factory": "1.*",
        "psr/log": "3.*",
        "psr/simple-cache": "3.*",
        "respect/validation": "2.*",
        "solital/katrina": "2.*",
        "spatie/db-dumper": "^3.4",
        "symfony/polyfill-php83": "^1.28",
        "symfony/var-dumper": "6.*",
        "symfony/var-exporter": "6.*",
        "symfony/yaml": "6.*",
        "vlucas/phpdotenv": "5.*"
    },
    "replace": {
        "solital/vinci-console": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Solital\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Solital\\Test\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
