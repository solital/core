{
    "name": "solital/core",
    "description": "Official core of Solital framework",
    "license": "MIT",
    "homepage": "http://solitalframework.rf.gd/",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "brenno-duarte/modern-php-exception": "2.*",
        "brenno-duarte/php-secure-password": "2.*",
        "codedungeon/php-cli-colors": "1.*",
        "delight-im/http": "^2.1",
        "egulias/email-validator": "^3.1",
        "fig/event-dispatcher-util": "^1.3",
        "matthiasmullie/minify": "^1.3",
        "phpmailer/phpmailer": "^6.5",
        "psr/cache": "3.*",
        "psr/container": "2.*",
        "psr/event-dispatcher": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/log": "3.*",
        "psr/simple-cache": "3.*",
        "react/event-loop": "1.2.0",
        "respect/validation": "^2.2",
        "solital/katrina": "2.*",
        "symfony/var-dumper": "^5.4",
        "symfony/var-exporter": "^5.4",
        "symfony/yaml": "^5.4",
        "vlucas/phpdotenv": "^5.4"
    },
    "replace": {
        "solital/vinci-console": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Solital\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Solital\\Test\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
